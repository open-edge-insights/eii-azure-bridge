{
    "ia_video_ingestion": {
        "version": "1.0",
        "type": "docker",
        "status": "running",
        "restartPolicy": "always",
        "settings": {
            "image": "${DOCKER_REGISTRY}openedgeinsights/ia_video_ingestion:$EII_VERSION",
            "createOptions": {
                "Hostname": "ia_video_ingestion",
                "Env": [
                    "AppName=VideoIngestion",
                    "DEV_MODE=$DEV_MODE",
                    "PROFILING_MODE=$PROFILING_MODE",
                    "no_proxy=localhost,127.0.0.1,${RTSP_CAMERA_IP},$ETCD_HOST",
                    "ETCD_HOST=$ETCD_HOST",
                    "ETCD_CLIENT_PORT: $ETCD_CLIENT_PORT",
                    "ETCD_PREFIX=$ETCD_PREFIX",
                    "PIPELINE: $PIPELINE",
                    "GENICAM=Matrix_Vision",
                    "GST_DEBUG=0,gencamsrc:2",
                    "AML_TENANT_ID=$AML_TENANT_ID",
                    "AML_PRINCIPAL_ID=$AML_PRINCIPAL_ID",
                    "AML_PRINCIPAL_PASS=$AML_PRINCIPAL_PASS"
                ],
                "NetworkingConfig": {
                    "EndpointsConfig": {
                        "host": {}
                    }
                },
                "HostConfig": {
                    "NetworkMode": "host",
                    "Devices": [
                        {
                            "PathOnHost": "/dev/",
                            "PathInContainer": "/dev/",
                            "CgroupPermissions": "rwm"
                        }
                    ],
                    "DeviceCgroupRules": [
                        "c 189:* rmw",
                        "c 209:* rmw",
                        "a 189:* rwm"
                    ],
                    "Binds": [
                        "/dev:/dev:rw",
                        "$EII_INSTALL_PATH/sockets:/EII/sockets:rw",
                        "/var/tmp:/var/tmp:rw",
                        "/tmp:/tmp:rw",
                        "/run/udev:/run/udev:ro",
                        "$EII_CERTIFICATES/rootca:/run/secrets/rootca",
                        "$EII_CERTIFICATES/VideoIngestion:/run/secrets/VideoIngestion"
                    ],
                    "LogConfig": {
                        "Type": "",
                        "Config": {}
                    }
                }
            }
        }
    }
}
