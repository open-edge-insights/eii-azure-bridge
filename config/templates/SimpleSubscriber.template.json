{
    "SimpleSubscriber": {
        "version": "1.0",
        "type": "docker",
        "status": "running",
        "restartPolicy": "always",
        "settings": {
            "image": "${DOCKER_REGISTRY}ia_azure_simple_subscriber:${EIS_VERSION}",
            "createOptions": {
                "User": "${EIS_UID}",
                "HostConfig": {
                    "ReadonlyRootfs": true
                }
            }
        }
    },
    "routes": {
        "BridgeToSimpleSubscriber": "FROM /messages/modules/EISAzureBridge/outputs/* INTO BrokeredEndpoint(\"/modules/SimpleSubscriber/inputs/input1\")"
    }
}
